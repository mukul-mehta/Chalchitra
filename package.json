    "material-ui": "^0.20.2",
    "materialize-css": "^1.0.0-rc.2",
    "moment": "2.24.0",
    "node-sass": "4.12.0",
    "nouislider": "14.0.2",
    "prop-types": "15.7.2",
    "react": "16.9.0",
    "react-animation": "^1.2.2",
    "react-datetime": "2.16.3",
    "react-dom": "16.9.0",
    "react-materialize": "^3.8.10",
    "react-render-html": "^0.6.0",
    "react-router-dom": "5.0.1",
    "react-scripts": "3.1.0",
    "react-share": "^4.1.0",
    "react-slick": "0.25.2",
    "react-spring": "^8.0.27",
