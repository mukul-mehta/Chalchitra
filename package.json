{
  "name": "material-kit-react",
  "version": "1.8.0",
  "description": "Material Kit React. Coded by Creative Tim",
  "private": true,
  "main": "dist/index.js",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
